{
    "manifest_version": 3,
    "name": "Delete History by Keyword",
    "author":"Jingfeng Xia",
    "version": "1.0",
    "description": "Delete browsing history based on keywords",
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "icon16.png",
            "32": "icon32.png"
        }
    },
    "permissions": ["history"],
    "background": {
        "service_worker": "background.js"
    }
}